{
    "name": "@template/demo",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "env-cmd -x vite --port=\\$PORT --host",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --max-warnings 0",
        "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
        "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
        "preview": "vite preview",
        "serve": "env-cmd -x serve -s dist --listen=\\$PORT",
        "cypress": "cypress run --browser chromium",
        "cypress:open": "cypress open",
        "test:prepare": "pnpm run build && pnpm run serve",
        "test:e2e": "env-cmd -x start-server-and-test test:prepare \\$BASE_URL cypress",
        "test:component": "pnpm run cypress --component",
        "test": "vitest"
    },
    "dependencies": {
        "@emotion/react": "^11.13.0",
        "@emotion/styled": "^11.13.0",
        "@mui/icons-material": "^5.16.5",
        "@mui/lab": "5.0.0-alpha.143",
        "@mui/material": "^5.16.5",
        "@mui/x-charts": "^7.11.0",
        "@mui/x-data-grid": "^6.20.4",
        "@mui/x-data-grid-pro": "^7.11.0",
        "@mui/x-date-pickers-pro": "^7.11.0",
        "@mui/x-license-pro": "^6.10.2",
        "@reduxjs/toolkit": "^1.9.7",
        "@tanstack/react-query": "^4.36.1",
        "@tanstack/react-query-devtools": "^4.36.1",
        "@template/common": "workspace:*",
        "axios": "^1.7.2",
        "dayjs": "^1.11.12",
        "formik": "^2.4.6",
        "i18next": "^23.12.2",
        "i18next-browser-languagedetector": "^7.2.1",
        "i18next-http-backend": "^2.5.2",
        "lodash": "^4.17.21",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-helmet": "^6.1.0",
        "react-hot-toast": "^2.4.1",
        "react-i18next": "^13.5.0",
        "react-redux": "^8.1.3",
        "react-router-dom": "^6.25.1",
        "redux": "^4.2.1",
        "yup": "^1.4.0"
    },
    "devDependencies": {
        "@types/lodash": "^4.17.7",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^7.17.0",
        "@typescript-eslint/parser": "^7.17.0",
        "@vitejs/plugin-react": "^4.3.1",
        "cypress": "^13.13.1",
        "cypress-image-diff-js": "^2.2.1",
        "cypress-real-events": "^1.13.0",
        "cypress-vite": "^1.5.0",
        "dotenv": "^16.4.5",
        "env-cmd": "^10.1.0",
        "eslint-plugin-paths": "^1.0.8",
        "jsdom": "^24.1.1",
        "start-server-and-test": "^2.0.4",
        "vite": "^4.5.3",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^2.0.4"
    }
}
